<style lang="scss" is:global>
aside {
    position: fixed;
    top: 0;
    display: none;
    width: 35vw;
    right: 0;
    height: 100vh;
}
.otter {
  position: absolute;
  overflow: visible;
  transition: all 0.2s ease-in-out;
  opacity: 0;

  path {
    transition: all 3s ease-in-out;
    transform-origin: 50% 50%;
  }
  circle {
    transition: all 0.2s ease-in-out;
  }
}
.visible {
  opacity: 1;
}
#frame-2-otter-in {
  right: 0;

  circle {
    transform: translateY(0);
    transition: 5s;
  }
  #otter-2 {
    transform: translate(0);
    transition: all 3s ease-in-out;
  }


  &.visible {

    circle {
      transform: translateY(-20vh);
    }
    #otter-2 {
      transform: translate(-15vw, 5vh);
    }
  }
}
#frame-4-otter-turning-2 {
  top: 40vh;
  left: -60vw;

  circle {
    transform: translateY(0);
    transition: 5s;
  }
  #otter-4 {
    transform: rotate(0deg);
  }


  &.visible {

    circle {
      transform: translateY(-15vh);
    }
      #otter-4 {
      transform: rotate(30deg);
    }
  }
}
#frame-5-otter-questing {
  bottom: 0;

  circle {
    transform: translateY(0);

    &:nth-child(odd) {
        transition: 5s;
    }
    &:nth-child(even) {
        transition: 7s;
    }
  }

  &.visible {

    circle {
      &:nth-child(odd) {
        transform: translateY(-10vh);
      }
      &:nth-child(even) {
        transform: translateY(-15vh);
      }
    }
  }
}
#frame-6-otter-surfacing-1 {
  top: 30vh;

  circle {
    transform: translateY(0);

    &:nth-child(odd) {
        transition: 3s;
    }
    &:nth-child(even) {
        transition: 6s;
    }
  }
  #otter-6 {
    transform: translate(0);
    animation-timing-function: ease-out;
  }
  &.visible {

    circle {
      &:nth-child(odd) {
        transform: translateY(-10vh);
      }
      &:nth-child(even) {
        transform: translateY(-15vh);
      }
    }
    #otter-6 {
      transform: translate(3vw, -15vh);
    }
  }
}
@media (min-width: 768px){
   aside {
    display: block;
  }
}
</style>
